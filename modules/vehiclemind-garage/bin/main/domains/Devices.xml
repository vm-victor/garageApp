<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">

  <module name="devices" package="com.vehiclemind.garage.db"/>

  <entity name="Devices" sequential="true" cacheable="true">
    <string name="uuid" required="true" />
    <many-to-one name="customer" ref="com.vehiclemind.garage.db.Customers" />
  </entity>
</domain-models>